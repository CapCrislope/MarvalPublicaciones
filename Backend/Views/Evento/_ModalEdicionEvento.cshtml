                        @Html.ValidationMessageFor(model => model.EdicionEvento.Hora)
                    </div>

                </div>
                <div class="row" style="margin-top:10px;">
                    <div class="col-md-4">
                        <div>
                            <label>Lugar</label>
                            @Html.EditorFor(model => model.EdicionEvento.Lugar, new { htmlAttributes = new { @class = "form-control input-sm" } })
