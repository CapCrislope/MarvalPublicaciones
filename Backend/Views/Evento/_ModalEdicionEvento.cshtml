                    <div class="row" style="margin-top:10px;">
                        <div class="col-md-5">
                            <label>Titulo URL</label>
                            @Html.EditorFor(model => model.EdicionEvento.Title_url, new { htmlAttributes = new { @class = "form-control input-sm" } })
