@model BackendPortal.Models.VM.TabVariosContactoVM
<div class="panel-body">
    <form id="TabContactoForm" onsubmit="GuardarTabContacto(); return false;">
        <div class="form-group">
            <div class="row">
                <div class="col-md-12">
                    <label for="contact_mails">Mails de contacto</label>
                    @Html.TextAreaFor(model => model.contactoDTO.contact_mails, new { cols = 60, rows = 10, @class = "form-control", @Style = "resize: none;" })
                    @Html.ValidationMessageFor(model => model.contactoDTO.contact_mails)
                </div>
            </div>
        </div>
        <div style="text-align:right;float:right;">
            <button type="submit" class="btn btn-primary">Guardar</button>
        </div>
    </form>
</div>
<div id="AlertsVarios">
</div>
@Scripts.Render("~/bundles/jqueryval")
