                                @Html.ValidationMessageFor(model => model.EdicionEvento.TituloEdicion)
                            </div>
                        </div>

                    </div>
                </div>



                <div class="row">
                    <div class="col-md-3">
                        <div>
                            <label>Fecha Evento Desde *</label>
                            <div class="input-group date">
                                @Html.EditorFor(model => model.EdicionEvento.Fecha, new { htmlAttributes = new { @class = "form-control input-sm", @readonly = "readonly" } })
