                        </div>
                    </div>
                    <div class="col-md-4">
                        <div>
                            <label>Formato en</label>
                            @Html.EditorFor(model => model.EdicionEvento.Formato_en, new { htmlAttributes = new { @class = "form-control input-sm" } })
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-4">

                        <div style="margin-top:20px;">
                            <label>Áreas de Práctica</label>
                            <div id="pnlAreasPracticas@{@identif}">
                                <img src="~/Images/loading.gif" />
                            </div>
                        </div>


                    </div>
                    <div class="col-md-4">
                        <div style="margin-top:20px;">
                            <label>Autores</label>
                            <div id="pnlAutores@{@identif}">
