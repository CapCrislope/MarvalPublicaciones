                                            @Html.EditorFor(model => model.EdicionEvento.TxtEvento, new { htmlAttributes = new { @class = "form-control input-sm", id = "EventoTxt", placeholder = "Nuevo evento..." } })
                                            @Html.ValidationMessageFor(model => model.EdicionEvento.TxtEvento)
