                        <div style="margin-top:20px;">
                            <label>Speakers</label>
                            <div id="pnlSpeakers@{@identif}">
                                <img src="~/Images/loading.gif" />
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row" style="margin-top:20px">
                    <div class="col-md-4">
                        <label>Archivo calendario Outlook ES</label>
                        <div class="input-group">
                            @Html.EditorFor(model => model.EdicionEvento.StrCalendario, new { htmlAttributes = new { @class = "form-control input-sm ta-coment", id = "inputStrCalendario", name = "StrCalendarioES", @readonly = "readonly" } })
                            <span class="input-group-btn">
                                <span class="btn btn-default btn-file btn-sm">
                                    Buscar…
                                    @Html.TextBoxFor(model => model.EdicionEvento.FCalendario, new { type = "file", id = "fCalendario", name = "", @class = "nodisplay", onchange = "SetNameFile('fCalendario')" })
                                </span>
                            </span>
                        </div>
                        @*<div id="divFormatoIncorrecto" class="hidden" style="margin-top:10px;">
                    <span class="field-validation-error">Formato de archivo incorrecto.</span>
                </div>*@
                    </div>
                    <div class="col-md-1" style="padding-left:0px;">
                        <button type="button" class="btn btn-sm btn-default" onclick="BorrarLinkES();" title="Borrar archivo Outlook ES" style="margin-top:22px;">
                            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                        </button>
                    </div>
                    <div class="col-md-4">
                        <label>Archivo calendario Outlook EN</label>
                        <div class="input-group">
                            @Html.EditorFor(model => model.EdicionEvento.StrCalendarioEn, new { htmlAttributes = new { @class = "form-control input-sm ta-coment", id = "inputStrCalendarioEn", name = "StrCalendarioEN", @readonly = "readonly" } })
                            <span class="input-group-btn">
                                <span class="btn btn-default btn-file btn-sm">
                                    Buscar…
                                    @Html.TextBoxFor(model => model.EdicionEvento.FCalendario_en, new { type = "file", id = "fCalendario_en", name = "", @class = "nodisplay", onchange = "SetNameFile('fCalendario_en')" })
                                </span>
                            </span>
                        </div>
                        @*<div id="divFormatoIncorrecto_en" class="hidden" style="margin-top:10px;">
                    <span class="field-validation-error">Formato de archivo incorrecto.</span>
                </div>*@
                    </div>
                    <div class="col-md-1" style="padding-left:0px;">
                        <button type="button" class="btn btn-sm btn-default" onclick="BorrarLinkEN();" title="Borrar archivo Outlook EN" style="margin-top:22px;">
                            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                        </button>
                    </div>
                    <div class="col-md-2">
                        <div style="margin-top:22px;">
                            @Html.CheckBoxFor(model => model.EdicionEvento.ConConfirmacion, new { @checked = "checked", @Value = "true" }) <label>Confirmación</label>
                        </div>
                    </div>
                </div>
                <div class="row" style="margin-top:20px">
                    <div class="col-md-6">
                        <div>
                            <label>Link Google Calendar ES</label>
                            @Html.EditorFor(model => model.EdicionEvento.LinkGoogle, new { htmlAttributes = new { @class = "form-control input-sm" } })
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div>
                            <label>Link Google Calendar EN</label>
                            @Html.EditorFor(model => model.EdicionEvento.LinkGoogleEn, new { htmlAttributes = new { @class = "form-control input-sm" } })
                        </div>
                    </div>
                </div>
                <div class="row" style="margin-top:20px">
                    <div class="col-md-8">
                        <div>
                            <label>Link al Evento</label>
                            @Html.EditorFor(model => model.EdicionEvento.LinkEvento, new { htmlAttributes = new { @class = "form-control input-sm" } })
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                </div>

                <div style="border: solid 1px #ccc; padding: 12px; margin-top: 20px">
                    <div class="row">
                        <div class="col-md-6">
                            <label>APP</label>
                            <div>
                                @Html.CheckBoxFor(model => model.EdicionEvento.FijarApp, new { @checked = "checked", @Value = "true" }) <label>Fijar en APP</label>
                            </div>
                        </div>
                        <div class="col-md-6">

                        </div>

                    </div>
                </div>


                @*---------------------------------------------------------------------------------------------------------------------------------------*@
