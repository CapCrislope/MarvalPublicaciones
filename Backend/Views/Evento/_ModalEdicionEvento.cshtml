


@model BackendPortal.Models.VM.EdicionEventoVM

@{ string identif = "Modal";
    string rutaImg = @System.Configuration.ConfigurationManager.AppSettings["DIR_EVENTO_URL"] + Model.EdicionEvento.IdEdicionEvento + "/" + Model.EdicionEvento.Picture;
    string rutaImgFooter = @System.Configuration.ConfigurationManager.AppSettings["DIR_EVENTO_URL"] + Model.EdicionEvento.IdEdicionEvento + "/" + Model.EdicionEvento.PictureFooter;
}

<div id="AlertsModal">
</div>
<div class="modal-dialog modal-lg">
    <div class="modal-content">

        <form id="EdicionEventoForm" onsubmit="GuardarEdicionEvento(); return false" enctype="multipart/form-data">
            @Html.HiddenFor(model => model.Modo)
            @Html.HiddenFor(model => model.Titulo)
            @Html.HiddenFor(model => model.EdicionEvento.IdEdicionEvento)
            @Html.HiddenFor(model => model.EdicionEvento.AreasPracticas, new { id = "hAreasPracticas" + identif, @class = "IncludeHidden" })
            @Html.HiddenFor(model => model.EdicionEvento.Autores, new { id = "hAutores" + identif, @class = "IncludeHidden" })
            @Html.HiddenFor(model => model.EdicionEvento.Speakers, new { id = "hSpeakers" + identif, @class = "IncludeHidden" })
            @Html.HiddenFor(model => model.EdicionEvento.IdEvento, new { id = "hEvento" + identif, @class = "IncludeHidden" })

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">@Model.Titulo</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="contenedorFoto">
                                <label>Imagen principal</label>
                                <center>
                                    <div style="margin-top:10px;">
                                        @if (!string.IsNullOrEmpty(Model.EdicionEvento.Picture) && BackendPortal.Controllers.BaseController.ExisteUrl(rutaImg))
                                        {
                                            <img id="imgPicture" src="@{@rutaImg}" title="@{@Model.EdicionEvento.Picture_description}" class="imgFoto" />
                                        }
                                        else
                                        {
                                            <img id="imgPicture" src="~/Images/imgNoDisp.png" title="Imagen no disponible" class="imgFoto" />
                                        }
                                    </div>
                                </center>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-9">
                                        <div class="input-group">
                                            @Html.EditorFor(model => model.EdicionEvento.Picture, new { htmlAttributes = new { @class = "form-control input-sm ta-coment", id = "principalPicture", name = "", @readonly = "readonly" } })
                                            <span class="input-group-btn">
                                                <span class="btn btn-default btn-file btn-sm">
                                                    Buscar…
                                                    @Html.TextBoxFor(model => model.EdicionEvento.PictureUpload, new { type = "file", id = "inputFilePicture", name = "", @class = "nodisplay", onchange = "SetNameFile('inputFilePicture');readURL(this, 'imgPicture');" })
                                                </span>
                                            </span>
                                        </div>
                                        <div id="divFormatoIncorrecto" class="hidden" style="margin-top:10px;">
                                            <span class="field-validation-error">Formato de archivo incorrecto.</span>
                                        </div>
                                        @Html.ValidationMessageFor(model => model.EdicionEvento.Picture)
                                    </div>
                                    <div class="col-md-3">
                                        <button type="button" class="btn btn-sm btn-default" onclick="BorrarPicturePrincipal();">Limpiar</button>
                                    </div>
                                </div>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-9">
                                        <label>Descripción</label>
                                        @Html.EditorFor(model => model.EdicionEvento.Picture_description, new { htmlAttributes = new { @class = "form-control input-sm" } })
                                        @Html.ValidationMessageFor(model => model.EdicionEvento.Picture_description)
                                    </div>
                                    <div class="col-md-3">
                                    </div>
                                </div>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-9">
                                        @Html.HiddenFor(model => model.EdicionEvento.ImagenPosY, new { id = "hImagenPosY" + identif, @class = "IncludeHidden" })

                                        <div class="form-group">
                                            <label for="ddlImagenPosY@{@identif}">Posicion imagen</label>
                                            <div id="pnlImagenPosY@{@identif}"></div>
                                            @Html.ValidationMessageFor(model => model.EdicionEvento.ImagenPosY)
                                        </div>
                                    </div>
                                    <div class="col-md-3"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="contenedorFoto">
                                <label>Imagen Footer</label>
                                <center>
                                    <div style="margin-top:10px;">
                                        @if (!string.IsNullOrEmpty(Model.EdicionEvento.PictureFooter) && BackendPortal.Controllers.BaseController.ExisteUrl(rutaImgFooter))
                                        {
                                            <img id="imgPictureFooter" src="@{@rutaImgFooter}" title="@{@Model.EdicionEvento.Picture_description}" class="imgFoto" />
                                        }
                                        else
                                        {
                                            <img id="imgPictureFooter" src="~/Images/imgNoDisp.png" title="Imagen no disponible" class="imgFoto" />
                                        }
                                    </div>
                                </center>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-9">
                                        <div class="input-group">
                                            @Html.EditorFor(model => model.EdicionEvento.PictureFooter, new { htmlAttributes = new { @class = "form-control input-sm ta-coment", id = "footerPicture", name = "", @readonly = "readonly" } })
                                            <span class="input-group-btn">
                                                <span class="btn btn-default btn-file btn-sm">
                                                    Buscar…
                                                    @Html.TextBoxFor(model => model.EdicionEvento.ImgFooter, new { type = "file", id = "inputFilePictureFooter", name = "", @class = "nodisplay", onchange = "SetNameFile('inputFilePictureFooter');readURL(this, 'imgPictureFooter');" })
                                                </span>
                                            </span>
                                        </div>
                                        <div id="divFormatoIncorrectoFooter" class="hidden" style="margin-top:10px;">
                                            <span class="field-validation-error">Formato de archivo incorrecto.</span>
                                        </div>
                                        @Html.ValidationMessageFor(model => model.EdicionEvento.PictureFooter)
                                    </div>
                                    <div class="col-md-3">
                                        <button type="button" class="btn btn-sm btn-default" onclick="BorrarPictureFooter();">Limpiar</button>
                                    </div>
                                </div>
                                <div class="row" style="margin-top:10px;">
                                    <div class="col-md-9">
                                        <label>Descripción</label>
                                        @Html.EditorFor(model => model.EdicionEvento.PictureFooter_description, new { htmlAttributes = new { @class = "form-control input-sm" } })
                                        @Html.ValidationMessageFor(model => model.EdicionEvento.PictureFooter_description)
                                    </div>
                                    <div class="col-md-3">
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="form-group" style="border: solid 1px #ccc; padding: 12px;">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group-sm">
                                <label for="ddEvento@{@identif}">Evento*</label>
                                <div class="row">
                                    <div class="col-md-2">
                                        <div style="text-align:center">
                                            @Html.RadioButtonFor(model => model.EdicionEvento.NuevoEvento, false, new { @Checked = "checked", id = "EventoCombo" })
                                        </div>
                                    </div>
                                    <div class="col-md-10">
                                        <div>
                                            <div id="pnlEvento@{@identif}">
